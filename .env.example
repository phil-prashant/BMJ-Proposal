# BMJ-Machinery Proposal Email Server - Environment Variables
# Copy this file to .env and fill in your actual values

# Resend API Configuration (Primary Email Service)
RESEND_API_KEY=re_your_api_key_here

# EmailJS Configuration (Fallback Email Service)
EMAILJS_SERVICE_ID=service_your_service_id
EMAILJS_TEMPLATE_ID=template_your_template_id
EMAILJS_PUBLIC_KEY=your_public_key_here

# Server Configuration
PORT=5000
